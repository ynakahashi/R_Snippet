get_eta <- function(b) {
eta <- X %*% b
return(eta)
}
get_mu <- function(b) {
mu <- exp(X %*% b) / (1 + exp(X %*% b))
return(mu)
}
get_var <- function(b) {
var <- get_mu(b) * (1 - get_mu(b))
return(var)
}
get_z <- function(b) {
p <- get_mu(b)
z <- get_eta(b) + (d$y - p) / (1/p + 1/(1-p))
return(z)
}
get_W <- function(b) {
t <- get_mu(b) * (1 - get_mu(b))
w <- t^2 / get_var(b)
return(w)
}
